{
  "name": "Deepbreath",
  "version": "0.2",
  "description": "DeepBreath Email Extension",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "https://language.googleapis.com/*",
    "storage"
  ],
  "web_accessible_resources": [
    "third_party/jquery-1.10.2.min.js",
    "third_party/gmail.js",
    "main.js"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 3
}
